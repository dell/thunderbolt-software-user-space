#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -pthunderbolt-software-user-space -Bbuild_userspace -Scmake -D. --

override_dh_auto_build:
	dh_auto_build -pthunderbolt-software-user-space -Bbuild_userspace

override_dh_auto_install:
	dh_auto_install --destdir debian/thunderbolt-software-user-space -Bbuild_userspace

override_dh_auto_clean:
	rm -rf build*
